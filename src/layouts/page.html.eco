---
layout: default
---


<article id="page" class="page">
	<header>
		<div class="row">
			<div class="columns">
				<h1><%= @document.title %></h1>
				<% if  @document.subtitle: %>
					<h3><%= @document.subtitle %></h3>
				<% end %>
			</div>
		</div>
	</header>
	<div class="row">
		<div class="columns page-content">
			<%- @content %>
		</div>
	</div>
</article>